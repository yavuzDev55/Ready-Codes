using UnityEngine.Audio;
using UnityEngine;

// Used as a prefab. Accessed via Audio Manager.

[System.Serializable]
public class sound
{
    public AudioClip clip;

    public string name;

    [Range(0f,1f)]
    public float volume;
    [Range(.3f,3f)]
    public float pitch;
    public bool loop;

    [HideInInspector]
    public AudioSource source;
}
